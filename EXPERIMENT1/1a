class PrimitiveDataTypes{
int primitiveInt;
double primitiveDouble;
char primitiveChar;
float primitiveFloat;
public static void main(String[] args){
PrimitiveDataTypes dDpt = new PrimitiveDataTypes();
System.out.println("default value of int:" + dDpt.primiti>
System.out.println("default value of double:" + dDpt.prim>
System.out.println("default value of char:" + dDpt.primit>
System.out.println("default value of float:" + dDpt.primi>
}
}
