class displaydefaultprimitivetype{
int primitiveInt;
double primitiveDouble;
char primitiveChar;
float primitiveFloat;
public static void main(String[] args){
displaydefaultprimitivetype dDpt=new displaydefaultprimitivetype();
System.out.println("default value of int:" + dDpt.primitiveInt);
System.out.println("default value of double:" + dDpt.primitiveDouble);
System.out.println("default value of char:" + dDpt.primitiveChar);
System.out.println("default value of float:" + dDpt.primitiveFloat);
}
}
